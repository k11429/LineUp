{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"account",
      "labels":{
         "app":"account"
      }
   },
   "spec":{
      "replicas":3,
      "selector":{
         "app":"account"
      },
      "template":{
         "metadata":{
            "labels":{
               "app":"account"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"account",
                  "image":"docker.io/k11429/team4-account:0.1",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":11001
                     }
                  ]
               }
            ]
         }
      }
   }
}
